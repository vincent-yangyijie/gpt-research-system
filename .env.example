# GPT-Researcher Configuration
# Copy this file to .env and fill in your API keys

# Google Gemini API Key (for FAST_LLM)
GOOGLE_API_KEY=your-gemini-api-key-here

# KIMI API Key (for SMART_LLM - Moonshot AI)
KIMI_API_KEY=your-kimi-api-key-here

# Tavily API Key (for web search functionality)
TAVILY_API_KEY=your-tavily-api-key-here

# Optional: OpenAI API Key (if you have one)
# OPENAI_API_KEY=your-openai-api-key-here

# LLM Configuration
FAST_LLM=google_genai:gemini-2.0-flash-exp
SMART_LLM=kimi:kimi-k2

# Embedding Configuration
EMBEDDING=openai:text-embedding-3-small

# Research configuration
RESEARCH_DEPTH=deep
MAX_SEARCH_RESULTS=10
